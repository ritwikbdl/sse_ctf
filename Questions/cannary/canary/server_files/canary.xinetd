service dumped
{
    disable        = no
    sockety_type   = stream
    protocol       = tcp
    wait           = no
    log_type       = FILE /var/log/xinetd.log
    log_on_success = HOST PID EXIT DURATION
    log_on_failure = HOST
    user           = root
    bind           = 0.0.0.0
    port           = 1340

    server         = /home/ctf/canary
    type           = UNLISTED

    per_source     = 2
    rlimit_cpu     = 1
}
